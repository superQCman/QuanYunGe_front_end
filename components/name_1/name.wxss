.archi-casca{
  margin-top: 16rpx;
}

.archi-casca-name {
  position: relative;
  left: 50%;
  transform: translateX(-50%);
  width: 430rpx;
}

.archi-desc {
  width: 100%;
  text-align: center;
  font-size: 18rpx;
  background: linear-gradient(90deg, #754ABC 15.08%, #009FE8 80.82%);
  background-clip: text;
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.logo{
  display: flex;
  justify-content: center;
  align-items: center;
  height:auto;
  width: auto;
}

.log .img{
  display: flex;
  justify-content: flex-start;
  margin-right: 0rpx;
  height: 50rpx;
  width: 950rpx;
}

.logo .img_1{
  display: flex;
  justify-content: flex-start;
  margin-right: 0rpx;
  height: 210rpx;
  width: 210rpx;
}

.logo .img_2{
  display: flex;
  justify-content: flex-start;
  margin-right: 35rpx;
